    "mocha": "^5.2.0"
  },
  "dependencies": {
    "npm-watch": "^0.4.0"
  },
  "watch": {
    "build": {
      "patterns": ["src/"],
      "extensions": "js,jsx,html,css",
      "ignore": "src/vendor/external.min.js",
      "quiet": true,
